<pre>
<?
	var fs = require('fs');
	fs.readFile(__dirname+'/'+$_GET['file'],wrapper(function(err,data)
	{
		if(err) 
		{
			exit(err);
			return;
		}

		var str = data.toString();
		var c = str;
		var list = c.split('\n');
		for(var i=0;i<list.length;i++)
		{
			var line = ('00000'+(i+1)).slice(-4) + ' ';
			list[i] = line + list[i];
		}
		c = list.join('\n');
		str = c;
		str = str.replace(/\&/g,'&amp');
		str = str.replace(/\</g,'&lt;');
		echo(str);
	}));
?>
</pre>